<table id="CustomerTable">
    <thead>
        <tr>
            <th class="bg-success text-white">Customer #</th>
            <th class="bg-success text-white">First name</th>
            <th class="bg-success text-white">Last name</th>
            <th class="bg-success text-white">Email</th>
            <th class="bg-success text-white">Notes</th>
            <th class="bg-success text-white">Meal requirements</th>
            <th class="bg-success text-white">Edit</th>
			<th class="bg-success text-white">Delete</th>
        </tr>
    </thead>
    <tbody>
		<% if (typeof customers != 'undefined') { %>
	    	<% customers.forEach(function(customer){ %>
		    	<% include customer.ejs %>
		    <% }); %>
		<% } %>
	</tbody>
</table>